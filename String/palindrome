import java.util.*;

class Program {
  public static boolean isPalindrome(String str) {
    // Write your code here.
    if(str == null) return false;
    
    for(int i=0,j=str.length()-1; i<=j;i++,j--)
      {
        if(str.charAt(i) != str.charAt(j)) return false;
      }
    return true;
  }
}
